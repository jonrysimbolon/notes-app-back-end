{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "no-underscore-dangle": "off"
    },
    "spaced-comment": [ "error", "always", {
    "line": {
        "markers": ["#region", "#endregion", "region", "endregion"]
    }
}]
}
